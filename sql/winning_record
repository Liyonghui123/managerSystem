CREATE TABLE `winning_record` (
  `ID` int(11) NOT NULL AUTO_INCREMENT COMMENT '自增ID',
  `PRIZE_ID` varchar(50) DEFAULT NULL COMMENT '奖品ID',
  `REDEEM_CODE` varchar(50) DEFAULT NULL COMMENT '兑换码',
  `PRIZE_NAME` varchar(50) DEFAULT NULL COMMENT '奖品名',
  `USER_ID` varchar(50) DEFAULT NULL COMMENT '中奖用户ID',
  `USER_NAME` varchar(50) DEFAULT NULL COMMENT '中奖用户名称',
  `PRIZE_NUM` int(11) DEFAULT NULL COMMENT '奖品数量',
  `CREATE_TIME` datetime DEFAULT CURRENT_TIMESTAMP COMMENT '中奖时间',
  `FIELD1` varchar(255) DEFAULT NULL COMMENT '备用字段1',
  `FIELD2` varchar(255) DEFAULT NULL COMMENT '备用字段2',
  `FIELD3` varchar(255) DEFAULT NULL COMMENT '备用字段3',
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB AUTO_INCREMENT=243 DEFAULT CHARSET=utf8