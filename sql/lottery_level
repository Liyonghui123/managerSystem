CREATE TABLE `lottery_level` (
  `LEVEL_ID` int(11) NOT NULL AUTO_INCREMENT COMMENT '自增ID',
  `LOTTERY_ID` varchar(50) DEFAULT NULL COMMENT '抽奖活动编号',
  `USER_LEVEL` varchar(50) DEFAULT NULL COMMENT '用户等级',
  `LIMIT_TIME` int(11) DEFAULT NULL COMMENT '限制次数',
  PRIMARY KEY (`LEVEL_ID`)
) ENGINE=InnoDB AUTO_INCREMENT=10 DEFAULT CHARSET=utf8